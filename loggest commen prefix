class Solution(object):
    def longestCommonPrefix(self, strs):
        """
        :type strs: List[str]
        :rtype: str
        """
        if not  strs:
            return " "
        minlen=min(len(s) for s in strs)
        for i in range(minlen):
            c=strs[0][i]
            for s in strs[1:]:
                if(s[i] !=c):
                    return strs[0][:i]
        return strs[0][:minlen]
